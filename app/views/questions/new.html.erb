<h1>質問投稿</h1>

<% if @question.errors.any? %>
  <%= @question.errors.count %>件のエラーが発生しました
  <ul>
    <% @question.errors.full_messages.each do |message| %>
      <li><%= message %></li>
    <% end %>
  </ul>
<% end %>

<%= form_with model: @question do |f| %>

　<h4>タイトル</h4>
  <%= f.text_field :title %>
    
　<h4>本文</h4>
　<%= f.text_area :body %>
    
    
  <!--解決/未解決-->
    
　<!--下書き/公開-->

  
　<%= f.submit '投稿' %>
<% end %>