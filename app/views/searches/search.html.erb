<% if @model == 'user' %>
    <h3>Users search for "<%= @keyword %>"</h3>
  <%= render 'users/index', users: @users %>
<% elsif @model == 'article' %>
    <h3>Article search for "<%= @keyword %>"</h3>
  <%= render 'articles/index', articles: @articles %>
<% elsif @model == 'word' %>
    <h3>Word search for "<%= @keyword %>"</h3>
  <%= render 'words/index', words: @words %>
<% elsif @model == 'question' %>
    <h3>Question search for "<%= @keyword %>"</h3>
  <%= render 'questions/index', question: @questions %>
<% end %>